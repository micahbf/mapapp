{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "quotemark": [true, "single", "avoid-escape"],
        "semicolon": "never",
        "trailing-comma": false,
        "variable-name": {
            "options": [
                "allow-leading-underscore",
                "allow-pascal-case",
                "ban-keywords",
                "check-format",
                "require-const-for-all-caps"
            ]
        }
    },
    "rulesDirectory": []
}
