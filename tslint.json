{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "interface-name": false,
    "no-console": false,
    "no-namespace": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "avoid-escape"],
    "semicolon": [true, "never"],
    "trailing-comma": false,
    "variable-name": {
      "options": [
        "allow-leading-underscore",
        "allow-pascal-case",
        "ban-keywords",
        "check-format",
        "require-const-for-all-caps"
      ]
    }
  },
  "rulesDirectory": []
}
